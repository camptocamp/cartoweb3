; if false, will outline only one shape at a time (default true)
multipleShapes = true

; if true, will ask for a label after shape is drawn (default true)
labelMode = true

; modify tools weights to change icons display order in the toolbar
; use negative weights to disable some statictools
;weightOutlinePoint = "-1"  ;default 70
;weightOutlineLine = "-1"  ;default 71
;weightOutlineRectangle = "-1"  ;default 72
;weightOutlinePoly = "-1"  ;default 73
