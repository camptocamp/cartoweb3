  LAYER
    NAME "mountain"
    TYPE POINT
    DATA "elevp"
    TEMPLATE "ttt"
    METADATA
        "id_attribute_string" "OGC_FID|string"
        "query_returned_attributes" "ZV2" 
    END
    CLASS
      NAME "> 3500m"
      EXPRESSION ([ZV2]>3500)
      STYLE
        COLOR 0 0 0
        SYMBOL "triangle"
        SIZE 12
      END
    END
    CLASS
      NAME "> 2500m"
      EXPRESSION ([ZV2]>2500 AND [ZV2]<3500)
      STYLE
        COLOR 0 0 0
        SYMBOL "triangle"
        SIZE 9
      END
    END
  END

  LAYER
    NAME "mountain_hilight"
    TYPE POINT
    DATA "elevp"
    TRANSPARENCY 50
    CLASS
      NAME "> 3500m"
      EXPRESSION ([ZV2]>3500)
      STYLE
        COLOR 255 0 104
        SYMBOL "triangle"
        SIZE 12
      END
    END
    CLASS
      NAME "> 2500m"
      EXPRESSION ([ZV2]>2500 AND [ZV2]<3500)
      STYLE
        COLOR 255 0 104
        SYMBOL "triangle"
        SIZE 9
      END
    END
  END
  
