MAP
  NAME "demoPlugins"
  #EXTENT 142776 4757216 1083857 5409261
  EXTENT -2000000 2000000 4000000 8000000
  IMAGETYPE PNG
  IMAGECOLOR 255 255 255
  STATUS ON
  UNITS METERS
  FONTSET "fonts.txt"
  SYMBOLSET "symbols.txt"
  SHAPEPATH "data"

# DEBUG capabilities
#
  CONFIG "MS_ERRORFILE" "/var/tmp/cw3_mapserver.log"
  DEBUG 2

# WFS DEBUG
#
   WEB
    IMAGEPATH "@CARTOCLIENT_GENERATED_FILE@"
    # Required WEB object parameters:
	# IMAGEPATH - Must be specified since this directory is used to store the temporary files downloaded from a WFS server.
    # By default now this is dynamically replaced by cw3setup and point to webroot/htdocs/generated/images
    # @FIXME : is there a better way to handle this restriction
    #IMAGEURL "images"
   END 
# END WFS DEBUG

  OUTPUTFORMAT
    NAME png
    DRIVER "GD/PNG"
    MIMETYPE "image/png"
    IMAGEMODE PC256
    EXTENSION "png"
    FORMATOPTION "INTERLACE=OFF"
    TRANSPARENT ON
  END
 
  OUTPUTFORMAT
    NAME jpeg
    DRIVER "GD/JPEG"
    MIMETYPE "image/jpeg"
    IMAGEMODE RGB
    EXTENSION "jpg"
    FORMATOPTION "INTERLACE=OFF"
    FORMATOPTION "QUALITY=80"
  END
 
  REFERENCE
    IMAGE images/keymap.png
    EXTENT 142776 4757331 1083562 5409260
    STATUS ON
    COLOR -1 -1 -1
    OUTLINECOLOR 200 200 200
    SIZE 140 97
  END

  SCALEBAR
    POSTLABELCACHE TRUE
    STYLE 0
    UNITS kilometers
    SIZE 350 3
    TRANSPARENT TRUE
    COLOR 77 77 88
    IMAGECOLOR 242 255 195
    BACKGROUNDCOLOR 222 222 222
    LABEL
      TYPE BITMAP
      SIZE SMALL
      COLOR 0 0 0
      POSITION UR
      BUFFER 10
    END
  END

  PROJECTION
    'proj=utm'
    'zone=32'
    'ellps=WGS84'
    'datum=WGS84'
    'units=m'
    'no_defs'
  END

  LEGEND
    KEYSIZE 20 15
  END

  ##########
  # LAYERS #
  ##########
  LAYER
    NAME "raster"
    TYPE RASTER
    DATA "gtopo30_shade.tif"
    METADATA
