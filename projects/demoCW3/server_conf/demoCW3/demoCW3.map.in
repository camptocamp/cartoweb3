  #########
  # ROUTE #
  #########
  LAYER
    NAME "graph"
    TYPE LINE
    TRANSPARENCY 80
    CONNECTIONTYPE postgis
        CONNECTION "user=@DB_USER@ password=@DB_PASSWD@ host=@DB_HOST@ dbname=@DB_NAME@"
        DATA "the_geom from (SELECT the_geom from routing_results) as foo using unique gid using srid=-1"
    TEMPLATE "t"
    CLASS
      NAME "0"
      STYLE
        SYMBOL "circle"
        SIZE 10
        COLOR 90 27 191
      END
    END
  END

