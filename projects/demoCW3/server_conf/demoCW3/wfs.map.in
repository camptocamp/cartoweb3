# WFS SERVEURS #

LAYER
  NAME "wfscontinents"
  TYPE POLYGON
  STATUS ON
  CONNECTION "http://demo.mapserver.org/cgi-bin/wfs?"
  CONNECTIONTYPE WFS
  TEMPLATE "wfs-continent"
  METADATA
    "wfs_srs"             	"EPSG:4326"
    "wfs_typename"          "continents"
    "wfs_version"           "1.0.0"
    "wfs_connectiontimeout" "60"
    "wfs_maxfeatures"       "10"
	#XML "wfs_exceptions_format" "application/vnd.ogc.se_xml"
	"wfs_cache_dir" "/var/tmp/"
	"wfs_tmpdir" "/var/tmp/" 
  END
  PROJECTION
    "init=epsg:4326"
  END
  CLASS
    NAME "WFS Continents"
    STYLE
      COLOR 255 128 128
      OUTLINECOLOR 96 96 96
    END
  END
END # Layer

LAYER
  NAME "wfscities"
  TYPE POINT
  STATUS ON
  CONNECTION "http://demo.mapserver.org/cgi-bin/wfs?"
  CONNECTIONTYPE WFS
  TEMPLATE "wfs-cities"
  METADATA
    "wfs_srs"             	"EPSG:4326"
    "wfs_typename"          "cities"
    "wfs_version"           "1.0.0"
    "wfs_connectiontimeout" "60"
    "wfs_maxfeatures"       "1000"
	"wfs_cache_dir" "/var/tmp/"
	"wfs_tmpdir" "/var/tmp/" 
	#XML "wfs_exceptions_format" "application/vnd.ogc.se_xml"
  END
  PROJECTION
    "init=epsg:4326"
  END
  CLASS
    NAME "WFS Cities"
    STYLE
		SYMBOL "circle"
		SIZE 16
		COLOR 255 255 255
#		OVERLAYSYMBOL "circle"
#		OVERLAYCOLOR 0 0 0
#		OVERLAYSIZE 16
    END
  END
END # Layer


